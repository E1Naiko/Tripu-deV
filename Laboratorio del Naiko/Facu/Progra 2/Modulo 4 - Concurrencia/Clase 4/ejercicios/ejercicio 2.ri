  {}
  {}
  proceso vaciarPapelDeBolsa
  comenzar
    mientras (HayPapelEnLaBolsa)
      depositarPapel
  fin
  {}
  {}
  {}
  proceso vaciarFloresDeBolsa
  comenzar
    mientras (HayFlorEnLaBolsa)
      depositarFlor
  fin
  {}
  {}
  {}
  proceso juntarPapel
  comenzar
    mientras (HayFlorEnLaEsquina)
      tomarFlor
  fin
  {}
  {}
  {}
  proceso juntarFlor
  comenzar
    mientras (HayFlorEnLaEsquina)
      tomarFlor
