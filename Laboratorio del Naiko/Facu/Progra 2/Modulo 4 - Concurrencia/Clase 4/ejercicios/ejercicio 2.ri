  {}
  {}
  {}
  proceso recorridoFlores(E origenAv: numero E origenCa: numero)
  variables
    av: numero
    ca: numero
  comenzar
    repetir 5
      Random(av, 1,5)
      Random(ca, 1,10)
      BloquearEsquina(av,ca)
      Pos(av,ca)
      juntarFlor
      Pos(origenAv,origenCa)
      LiberarEsquina(av,ca)
  fin
  {}
  {}
  {}
  proceso recorridoPapeles(E origenAv: numero E origenCa: numero)
  variables
    av: numero
    ca: numero
  comenzar
    repetir 5
      Random(av, 6,10)
      Random(ca, 1,9)
      BloquearEsquina(av,ca)
      Pos(av,ca)
      juntarPapel
      Pos(origenAv,origenCa)
      LiberarEsquina(av,ca)
  fin
  {}
  {}
  {}
  proceso final(E origenAv: numero E origenCa: numero E tipo: boolean)
  comenzar
    BloquearEsquina(10,10)
    Pos(10,10)
    si (tipo)
      vaciarPapelDeBolsa
    sino
      vaciarFloresDeBolsa
    Pos(origenAv, origenCa)
    LiberarEsquina(10,10)
  fin
